<table style="width:100%">
    <tr>
        <td><h1>Items</h1></td>
        <td style="float:right">
            <a href="/">
                <button type="button" class="btn btn-info" value="edit">Dashboard</button>
            </a>
        </td>
    </tr>
</table>
<table class="table table-striped">
<thead>
    <tr>
    <th scope="col">Name</th>
    <th scope="col">Donor</th>
    <th scope="col">Quantity</th>
    <th scope="col">Received Date</th>
    <th scope="col">Expiry Date</th>
    <th scope="col">Price</th>
    </tr>
</thead>
<% @items.each do |item| %>
<tr>
    <td><%= item.name %></td>
    <td><%= item.donor %></td>
    <td><%= item.quantity %></td>
    <td><%= item.received %></td>
    <td><%= item.expiry %></td>
    <td><%= item.price %></td>
</tr>
<% end %>
</table>
<a class="navbar-link" href="/items/new"><button type="button" class="btn btn-warning" value="edit">Add New Item</button></a>